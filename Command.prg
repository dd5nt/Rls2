PROC ON_FILL;
  PRG_START(COMMAND.ON_M54_M55);
  PRG_START(COMMAND.ON_M61);
  PRG_START(COMMAND.ON_M52);
  PRG_START(COMMAND.WAIT_CONVEYOR_FILL);
ENDPROC

PROC OFF_FILL;
  PRG_START(COMMAND.OFF_M54_M55);
  PRG_START(COMMAND.OFF_M61);
  PRG_START(COMMAND.OFF_M52);
  PRG_START(COMMAND.OFF_CONVEYOR_FILL);
ENDPROC

PROC ON_M54_M55;
  OFF(M55);
  ON(M54);
  WAIT(,TIMER_M54_M55,TIME_M54_M55);
  OFF(M54);
  ON(M55);
  WAIT(,TIMER_M54_M55,TIME_M54_M55);
  LOOP;  
ENDPROC

PROC ON_M54&M55;
    ON(M54);
    ON(M54);
ENDPROC

PROC OFF_M54_M55;
  PRG_STOP(COMMAND.ON_M54_M55);
  OFF(M54);
  OFF(M55);
ENDPROC

