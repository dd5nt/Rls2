PROC ON_FILL_LOV;
   OFF(M51);
   ON(YA52);
   WAIT(SL509 || SL508 || SL507 || SL506,,);
   PRG_START(COMMAND.ON_M54_M55);
   WAIT(SL507 || SL506, TIMER_WAIT_SL507, TIME_WAIT_SL507);
   IF(SL507 || SL506)
     OFF(YA52);
   END;
   IF(! SL507 && ! SL506)
     MSG(0000, Error: Wait 30m and not SL507);
     OFF(YA52);
   END;
ENDPROC

PROC OFF_FILL_LOV;
  PRG_STOP(COMMAND.ON_FILL_LOV);
  OFF(YA52);
ENDPROC

