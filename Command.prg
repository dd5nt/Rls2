PROC WAIT_CONVEYOR_FILL;
  PRG_START(COMMAND.ON_CONVEYOR_FILL);
  WAIT(SL13);
  PRG_START(COMMAND.OFF_CONVEYOR_FILL);
ENDPROC

PROC ON_CONVEYOR_FILL;
  ON(M22);
  ON(M24);
  ON(M25);
ENDPROC

PROC OFF_CONVEYOR_FILL;
  PRG_STOP(COMMAND.WAIT_CONVEYOR_FILL);
  OFF(M22);
  OFF(M24);
  OFF(M25);
ENDPROC

